#include <stdio.h>
void main()
{
    int a[3][3],r,c,i,j;
    printf("Enter rows,columns of 1st matrix:");
    scanf("%d%d",&r,&c);
    printf("\nEnter the elements of 1st matrix ");
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
       { scanf("%d",&a[i][j]);
       }
    }
    for(i=0;i<r;i++)
    {for(j=0;j<c;j++)
    {printf("%d",a[i][j]);
    }
    
    printf("\n");}
     
    int b[i][j];
     printf("Enter rows,columns of 2nd matrix:");
    scanf("%d%d",&r,&c);
    printf("\nEnter the elements of 2nd matrix ");
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
       { scanf("%d",&b[i][j]);
       }
    }
    for(i=0;i<r;i++)
    {for(j=0;j<c;j++)
    {printf("%d",b[i][j]);
    } printf("\n");
    }
    printf("%d+%d",a[i][j]+b[i][j]);
    printf("\n");
}
