#include <stdio.h>

int main()
{
    int m1, m2, m3, m4, m5; 
    float per; 

    printf("Enter five subjects marks: ");
    scanf("%d%d%d%d%d", &m1, &m2, &m3, &m4, &m5);


    per = (m1 + m2 + m3 + m4 + m5) / 5.0;

    printf("Percentage = %.2f\n", per);
    
    if(per >= 70)
    {
        printf("distiction");
    }
    else if(per >= 60)
    {
        printf("firstclass");
    }
    else if(per >= 40)
    {
        printf("secondclass");
    }
    else
    {
        printf("failed");
    }

    return 0;
}
