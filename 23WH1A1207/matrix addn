#include<stdio.h>
void main()
{
int A[10][10],r1,c1,r2,c2,i,j,B[10][10],C[10][10];
printf(" enter the r1 ,c1" );
scanf("%d%d",&r1,&c1);
printf("Enter matrix A ");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
scanf("%d",&A[i][j]);
}
 }
 printf("matrix A\n");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
printf("%3d",A[i][j]);
}
printf("\n");

}
printf(" enter the r2 ,c2" );
scanf("%d%d",&r2,&c2);
printf("Enter matrix B ");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
scanf("%d",&B[i][j]);
}
}
 printf("matrix B\n");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
printf("%3d",B[i][j]);
}
printf("\n");
}
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
C[i][j]= A[i][j]+B[i][j];
}
}
 printf("matrix C\n");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
printf("%3d",C[i][j]);
}
printf("\n");
}
}
