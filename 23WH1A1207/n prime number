#include <stdio.h>

int main() {
    int i,j, num, c=0;
    printf("enter a number :");
    scanf("%d",&num);
    for(i=2;i<=num;i++)
    {
        for(j=2;j<i;j++)
        {
        c=2;
        if((i%j)==0)
        {
            c=0;
    break;
        }
        }
    if(c==2)
    printf("\n %d  ",i);
    }
    

    return 0;
}
