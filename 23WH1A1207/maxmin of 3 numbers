#include <stdio.h>
void main()
{
    int a,b,c;
printf("\n Enter three values :");
scanf("%d%d%d",&a,&b,&c);
if ( a>b && a>c)
printf("\n %d is maximum value ",a);
else if (b>a && b>c)
printf("\n %d is maximum value",b);
else
printf("\n %d is maximum value ",c );

if ( a<b && a<c)
printf("\n %d is minimum  value ",a);
else if (b<a && b<c)
printf("\n %d is minimum  value ",b);
else
printf("\n %d is minimum value",c );
}
