#include <stdio.h>
#include<stdlib.h>

void main() 
{
    int *a,i;
    a=(int*)calloc(5,sizeof(int));
    printf("Enter elements ");
    for(i=0;i<5;i++)
    {
        scanf("%d",a+i);
        printf("%d\n",*(a+i));
    }
    a=(int*)calloc(10,sizeof(int));
    for(i=0;i<10;i++)
    printf("%d\n",*(a+i));
}
