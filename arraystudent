#include<stdio.h>
struct student
{ char name[30];
 int rollno;
int cgpa;
};
void main()
{ struct student std[3];
int i;
for(i=0;i<3;i++)
{
printf("enter name,rollno,cgpa");
scanf("\n%s %d %d",std[i].name,&std[i].rollno,&std[i].cgpa);
}
for(i=0;i<3;i++)
{ printf("\n%s %d %d",std[i].name,std[i].rollno,std[i].cgpa);
}
}
