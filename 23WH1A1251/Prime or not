// Online C compiler to run C program online
#include <stdio.h>
void main ()
{
   int num,i;
   printf("Enter a number: ");
   scanf("%d",&num);
   for(i=2;i<num;i++)
   {
       if (num % i == 0)
   {
       printf("Number is not prime");
       break;
   }
   else 
   {
       printf("Number is prime");
       break;
   }
   } 
}
