  void array(int x[],int n)
 {
         int i;
         for(i=0;i<n;i++)
  {     scanf("%d",&x[i]);
         printf("%d ",x[i]);
  }
 }

  void sumarray(int x[],int n)
{
        int i,sum=0;
         for(i=0;i<n;i++)
 {
         sum=sum +x[i];
 }
         printf("%d",sum);
}

